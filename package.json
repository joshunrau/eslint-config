{
  "name": "@joshunrau/eslint-config",
  "type": "module",
  "version": "4.1.0",
  "packageManager": "pnpm@8.15.3",
  "description": "Linting configurations for my personal projects",
  "author": "Joshua Unrau",
  "license": "MIT",
  "homepage": "https://github.com/joshunrau/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshunrau/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/joshunrau/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "main": "./src/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build:lib": "tsc",
    "format": "prettier --write src",
    "lint": "pnpm exec tsc --noEmit --emitDeclarationOnly false && eslint --fix src",
    "prepare": "husky"
  },
  "peerDependencies": {
    "eslint": "8.x",
    "typescript": "5.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "eslint-plugin-jsdoc": "^48.2.2",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.7.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.0.0",
    "typescript-eslint": "^7.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@joshunrau/prettier-config": "^0.2.1",
    "@joshunrau/semantic-release": "^0.1.0",
    "@joshunrau/tsconfig": "^0.0.2",
    "@types/eslint": "^8.56.6",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.12.2",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "~5.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "extends": [
      "@joshunrau/semantic-release"
    ]
  }
}
